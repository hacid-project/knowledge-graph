    <!-- https://w3id.org/hacid/onto/top-level/hasBroader -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/hasBroader">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/associatedWith"/>
