    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/Continuum">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/DimensionalSpace"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace"/>
        <rdfs:comment xml:lang="en">A continuous dimensional space.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Continuum</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DataFormat -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DataFormat">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/Concept"/>
        <rdfs:comment xml:lang="en">A data format that can be used to serialize a dataset (e.g., CSV). Usage of controlled vocabularies is suggested.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Data Format</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DataGeneratingProcess -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DataGeneratingProcess">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="en">A process that generates some data as output.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Data Generating Process</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DataSource -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DataSource">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/InformationEntity"/>
        <rdfs:comment xml:lang="en">A data source is a location (in a broader sense, but typically an URL or a path in a specific file system) which contains somehow a dataset, alongside with the technical specification of how to access it. The hosted dataset should be available in one or more data formats.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Data Source</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/Dataset -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/Dataset">
        <owl:equivalentClass rdf:resource="https://w3id.org/hacid/onto/data/FiniteVariable"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Variable"/>
        <rdfs:comment xml:lang="en">A dataset is a variable (which can be composed of multiple sub-variables) whose extension is finitely expressable, i.e. a finite variable. This is an alias of data:FiniteVariable to be used when this alternative phrasing fits to the context  better.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DependentVariable -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DependentVariable">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Variable"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hacid/onto/data/IndependentVariable"/>
        <rdfs:comment xml:lang="en">A variable that is depedent on one or more (independent) variables in the given context of use.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Dependent Variable</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DerivedVariable -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DerivedVariable">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Variable"/>
        <rdfs:comment xml:lang="en">A variable that has been derived mathematically from one or more variables</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Derived Variable</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DimensionalSpace -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DimensionalSpace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/IndependentVariable"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hacid/onto/data/hasValuesOn"/>
                <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A dimension that can be somehow quantified, possibly structured by a system of measurement/classification. It can be bounded (e.g., a period of time) or not (e.g., the &quot;whole&quot; time), discrete (e.g, time by days) or continuous (e.g., a time continuum). It is a special case of an independent variable, specifically the most generic variable defined on the dimensional space. This enables a simple representation when the context of use does not require the distinction between variable and dimensional space.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Dimensional Space</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/DimensionalSpace"/>
        <rdfs:comment xml:lang="en">A dimensional space composed of discrete values. It could be inherently discrete (e.g., the set of international recognized countries) or obtained discretizing a continuous dimensional space (e.g., by sampling or partitioning). It can still have infinite (but countable) values if it is not bounded (e.g., the time sampled by hours, streching indefinetely in one or both directions).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Discrete Dimensional Space</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/FiniteDimensionalSpace -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/FiniteDimensionalSpace">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/hacid/onto/data/BoundedDimensionalSpace"/>
                    <rdf:Description rdf:about="https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/BoundedDimensionalSpace"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/FiniteVariable"/>
        <rdfs:comment xml:lang="en">A finite dimensional space, i.e. a dimensional space that is both discrete and bounded.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Finite Dimensional Space</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/FiniteVariable -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/FiniteVariable">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Variable"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hacid/onto/top-level/hasPart"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hacid/onto/data/FiniteVariable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A finite variable, i.e. a variable whose set of possible values is finite. For an independent variable, it means the corresponding dimensional space is finite. For a depedent variable, it means the independent variables it depends on are finite.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Finite Variable</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/GeodeticPoint -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/GeodeticPoint">
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Point"/>
        <rdfs:comment xml:lang="en">A point in a geodetic dimensional space, i.e. a georaphic location of the Earth surface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Geodetic Point</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/GeodeticRegion -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/GeodeticRegion">
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Region"/>
        <rdfs:comment xml:lang="en">A region in a geodetic dimensional space, i.e. a georaphic area of the Earth surface (e.g., the area of a country).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Geodetic Region</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/IndependentVariable -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/IndependentVariable">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Variable"/>
        <rdfs:comment xml:lang="en">A variable that is indepedent (not dependent from other variables) in the given context of use.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Independent Variable</rdfs:label>
    </owl:Class>
    


