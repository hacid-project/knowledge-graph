    <!-- https://w3id.org/hacid/onto/cs/ClimateModel -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/cs/ClimateModel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="en">A representation of the Earth’s climate system. A model is used to simulate and understand the climate, including its variability and changes over time, by using a set of equations based on physical, chemical, and biological principles.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/cs"/>
        <rdfs:label xml:lang="en">Climate Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/cs/Dataset -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/cs/Dataset">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="en">A dataset in this context is a collection of data organized in tabular format, where dimensions (independent variables) include at least space (on a geospatial grid) and time. It also includes at least one dependent variable (or simply variable), but there can be more than one.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/cs"/>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
    </owl:Class>
    


