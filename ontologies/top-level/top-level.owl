    <!-- https://w3id.org/hacid/onto/top-level/associatedWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/associatedWith">
        <rdfs:comment xml:lang="en">A catch-all object property, useful for alignment and querying purposes.
It is declared as both transitive and symmetric, in order to reason an a maximal closure of associations between individuals.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">associated with</rdfs:label>
        <rdfs:label xml:lang="it">associato a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/atTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/atTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isTimeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/TemporalEntity"/>
        <rdfs:comment xml:lang="it">La relazione esistente tra una qualsiasi entità ed un'entità temporale.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation holding between an entity ad a temporal entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">al tempo</rdfs:label>
        <rdfs:label xml:lang="en">at time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/classifies -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/classifies">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isClassifiedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Concept"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="it">La proprietà utilizzata per affermare che un concetto classificava una certa entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property used to state that a concept classified a certain entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">classifica</rdfs:label>
        <rdfs:label xml:lang="en">classifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/defines -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/defines">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/usesConcept"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isDefinedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Description"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Concept"/>
        <rdfs:comment xml:lang="en">A relation between a Description and a Concept, e.g. a Workflow for a governmental Organization defines the Role 'officer', or 'the Italian Traffic Law defines the role Vehicle'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">defines</rdfs:label>
        <rdfs:label xml:lang="it">definisce</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/definesRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/definesRole">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/defines"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isRoleDefinedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Description"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Role"/>
        <rdfs:comment xml:lang="en">A relation between a description and a role, e.g. the recipe for a cake defines the role 'ingredient'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">defines role</rdfs:label>
        <rdfs:label xml:lang="it">definisce il ruolo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/definesTask -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/definesTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/defines"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isTaskDefinedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Description"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Task"/>
        <rdfs:comment xml:lang="en">A relation between a description and a task, e.g. the recipe for a cake defines the task 'boil'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">defines task</rdfs:label>
        <rdfs:label xml:lang="it">definisce il task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/describes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/describes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isDescribedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Description"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="it">La realazione per associare una descrizione all'entità oggetto di tale descrizione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation for associating a description with the entity being described.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">describe</rdfs:label>
        <rdfs:label xml:lang="en">describes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/directlyFollows -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/directlyFollows">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/follows"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/directlyPrecedes"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="it">L'intransitivo segue la relazione. Ad esempio, il martedì segue direttamente il lunedì. L'immediatezza della precedenza dipende dalla concettualizzazione del progettista.</rdfs:comment>
        <rdfs:comment xml:lang="en">The intransitive follows relation. For example, Tuesday directly follows Monday. Directness of precedence depends on the designer conceptualization.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">directly follows</rdfs:label>
        <rdfs:label xml:lang="it">segue direttamente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/directlyPrecedes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/directlyPrecedes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/precedes"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="it">L'intransitivo precede la relazione. Ad esempio, il lunedì precede direttamente il martedì. L'immediatezza della precedenza dipende dalla concettualizzazione del progettista.</rdfs:comment>
        <rdfs:comment xml:lang="en">The intransitive precedes relation. For example, Monday directly precedes Tuesday. Directness of precedence depends on the designer conceptualization.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">directly precedes</rdfs:label>
        <rdfs:label xml:lang="it">precede direttamente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/executesTask -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/executesTask">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/isClassifiedBy"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/isExecutedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Action"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Task"/>
        <rdfs:comment xml:lang="en">A relation between an action and a task, e.g. 'putting some water in a pot and putting the pot on a fire until the water starts bubbling' executes the task 'boiling'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">esegue il task</rdfs:label>
        <rdfs:label xml:lang="en">executes task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/follows -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hacid/onto/top-level/follows">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/hacid/onto/top-level/precedes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:comment xml:lang="en">A relation between entities, expressing a 'sequence' schema.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una relazione tra entità, che esprime uno schema di 'sequenza'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">follows</rdfs:label>
        <rdfs:label xml:lang="it">segue</rdfs:label>
    </owl:ObjectProperty>
    


