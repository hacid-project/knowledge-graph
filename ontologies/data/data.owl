    <!-- https://w3id.org/hacid/onto/data/RegularPeriodicBinning -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/RegularPeriodicBinning">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/PeriodicBinning"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/RegularBinning"/>
