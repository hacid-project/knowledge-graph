        <rdfs:comment xml:lang="en">An instant in time, i.e. a point in a temporal dimensional space.</rdfs:comment>
        <rdfs:label xml:lang="en">Instant</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/IntervalSampling -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/IntervalSampling">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Sampling"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Interval Sampling</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/PeriodicBinning -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/PeriodicBinning">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Binning"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Periodic Binning</rdfs:label>
