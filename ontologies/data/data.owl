    <!-- https://w3id.org/hacid/onto/data/Aggregation -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/Aggregation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/top-level/Concept"/>
        <rdfs:comment xml:lang="en">An aggregation of a dependent variable over a variable it depends on. While the actual application of the aggregation depends on a grouping (more in general a quantization) over the independent variable, the aggregation is agnostic to the chosen quantization. It represents just that an aggregation function is to be applied along that dimension.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Aggregation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/Binning -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/Binning">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/Quantization"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Binning</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/BoundedDimensionalSpace -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/BoundedDimensionalSpace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/DimensionalSpace"/>
        <rdfs:comment xml:lang="en">A dimensional space that has finite boundaries (does not extend indefinitely), although not necessarily discrete and so still holding possibly infinite values (e.g., a finite interval of time).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Bounded Dimensional Space</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hacid/onto/data/ConstantDimensionalSpace -->

    <owl:Class rdf:about="https://w3id.org/hacid/onto/data/ConstantDimensionalSpace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hacid/onto/data/DiscreteDimensionalSpace"/>
        <rdfs:comment xml:lang="en">A degenerate dimensional space consisting of a single value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">Constant Dimensional Space</rdfs:label>
    </owl:Class>
    


