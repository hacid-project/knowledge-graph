        <rdfs:comment xml:lang="en">Associates a dimensional space to another dimensional space it is based on, e.g., by structuring or specializing it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/data"/>
        <rdfs:label xml:lang="en">based on dimensional space</rdfs:label>
    </owl:ObjectProperty>
    


