        <rdfs:comment xml:lang="en">The relation holding between an entity and a collection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">ha collezione</rdfs:label>
        <rdfs:label xml:lang="en">has collection</rdfs:label>
