                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#time"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">end time</rdfs:label>
        <rdfs:label xml:lang="it">tempo di fine</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/identifier -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/identifier">
        <rdfs:comment xml:lang="it">Il valore letterale di un identificativo, ex. il codice WMO '6100218'.</rdfs:comment>
        <rdfs:comment xml:lang="en">The literal value of an identifier, e.g. the WMO code '6100218'.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">identificativo</rdfs:label>
        <rdfs:label xml:lang="en">identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/mediaType -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/mediaType">
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Media"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">A value for the media type, e.g. text/csv, rdf/xml, etc.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un valore per il tipo di supporto, ad es. text/csv, rdf/xml, ecc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">media type</rdfs:label>
        <rdfs:label xml:lang="it">tipo di supporto</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/name -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/name">
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Il letterale che rappresenta il nome di un'entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">The literal representing the name of an entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:label xml:lang="it">nome</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/startTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/startTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/time"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/TimeInterval"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#dateTime"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#time"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="en">start time</rdfs:label>
        <rdfs:label xml:lang="it">tempo di inizio</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/symbol -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/symbol">
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="en">Property used to specify a symbol. For instance the symbol of a unit of measure</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà  utilizzata per specificare un simbolo. Ad esempio il simbolo dell'unità di misura</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">simbolo</rdfs:label>
        <rdfs:label xml:lang="en">symbol</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/time -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/time">
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">La proprietà per associare la rappresentazione letterale del tempo a un'entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property for associating the literal representation of time to an entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">tempo</rdfs:label>
        <rdfs:label xml:lang="en">time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/value -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/value">
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">La rappresentazione letterale di un valore.</rdfs:comment>
        <rdfs:comment xml:lang="en">The literal representation of a value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">valore</rdfs:label>
        <rdfs:label xml:lang="en">value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hacid/onto/top-level/year -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hacid/onto/top-level/year">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hacid/onto/top-level/time"/>
        <rdfs:domain rdf:resource="https://w3id.org/hacid/onto/top-level/TemporalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment xml:lang="it">La proprietà per collegare il valore in gYear ad un istanza della classe TemporalEntity.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that relates a valued expressed a gYear to an instance of the class TemporalEntity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hacid/onto/top-level"/>
        <rdfs:label xml:lang="it">anno</rdfs:label>
        <rdfs:label xml:lang="en">year</rdfs:label>
    </owl:DatatypeProperty>
    


